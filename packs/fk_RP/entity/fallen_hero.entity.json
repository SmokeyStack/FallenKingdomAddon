{
	"format_version": "1.8.0",
	"minecraft:client_entity": {
		"description": {
			"identifier": "fk:fallen_hero",
			"materials": {
				"default": "entity"
			},
			"textures": {
				"default": "textures/entity/fallen_hero"
			},
			"geometry": {
				"default": "geometry.fallen_hero"
			},
			"animations": {
				"walk": "animation.fallen_hero.move",
				"look_at_target": "animation.fallen_hero.look_at_target"
			},
			"scripts": {
				"pre_animation": [
					"variable.tcos0 = (math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;"
				]
			},
			"animation_controllers": [
				{
					"walk": "controller.animation.fallen_hero.move"
				},
				{
					"look_at_target": "controller.animation.fallen_hero.look_at_target"
				}
			],
			"render_controllers": [
				"controller.render.fallen_hero"
			],
			"enable_attachables": true,
			"spawn_egg": {
				"base_color": "#651818",
				"overlay_color": "#186565"
			}
		}
	}
}