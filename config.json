{
    "author": "SmokeyStack",
    "name": "FallenKingdom",
    "packs": {
        "behaviorPack": "./packs/fk_BP",
        "resourcePack": "./packs/fk_RP"
    },
    "regolith": {
        "dataPath": "./packs/data",
        "filterDefinitions": {
            "texture_list": {
                "url": "github.com/Bedrock-OSS/regolith-filters",
                "version": "1.1.1"
            }
        },
        "profiles": {
            "build": {
                "export": {
                    "readOnly": false,
                    "target": "local"
                },
                "filters": [
                    {
                        "filter": "texture_list"
                    }
                ]
            },
            "default": {
                "export": {
                    "readOnly": false,
                    "target": "development"
                },
                "filters": []
            }
        }
    }
}